<?xml version="1.0" encoding="UTF-8"?>
<Element xmlns="urn:fdc:difi.no:2017:vefa:structure-1">
    

        <Term>cac:LegalMonetaryTotal</Term>
        <Name>DOCUMENT TOTALS</Name>
        <Description>A group of business terms providing the monetary totals for the Invoice.</Description>
        <Reference type="BUSINESS_TERM">BG-22</Reference>
        
        <Element>
            <Term>cbc:LineExtensionAmount</Term>
            <Name>Sum of Invoice line net amount</Name>
            <Description>Sum of all Invoice line net amounts in the Invoice. Must be rounded to maximum 2 decimals.</Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-106</Reference>
            <Reference type="RULE">BR-12</Reference>
            <Reference type="RULE">BR-CO-10-SG</Reference>

            
            
            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>
                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            
            <Value type="EXAMPLE">3800</Value>
        </Element>
        
        <Element>
            <Term>cbc:TaxExclusiveAmount</Term>
            <Name>Invoice total amount without GST</Name>
            <Description>The total amount of the Invoice without GST.</Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-109-GST</Reference>
            <Reference type="RULE">BR-13-GST-SG</Reference>
            <Reference type="RULE">BR-CO-13-GST-SG</Reference>

            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>
                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            <Value type="EXAMPLE">3600</Value>
        </Element>
        
        <Element>
            <Term>cbc:TaxInclusiveAmount</Term>
            <Name>Invoice total amount with GST</Name>
            <Description>The total amount of the Invoice with GST.</Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-112-GST</Reference>
            <Reference type="RULE">BR-14-GST-SG</Reference>
            <Reference type="RULE">BR-CO-15-GST-SG</Reference>

            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>
                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            <Value type="EXAMPLE">4500</Value>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cbc:AllowanceTotalAmount</Term>
            <Name>Sum of allowances on document level</Name>
            <Description>Sum of all allowances on document level in the Invoice.</Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-107</Reference>
            <Reference type="RULE">BR-CO-11-SG</Reference>

            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>
                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            <Value type="EXAMPLE">200</Value>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cbc:ChargeTotalAmount</Term>
            <Name>Sum of charges on document level</Name>
            <Description>Sum of all charges on document level in the Invoice.</Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-108</Reference>
            <Reference type="RULE">BR-CO-12-SG</Reference>

            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>
                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            <Value type="EXAMPLE">0</Value>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cbc:PrepaidAmount</Term>
            <Name>Paid amount</Name>
            <Description>The sum of amounts which have been paid in advance. </Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-113</Reference>

            
            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>

                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            <Value type="EXAMPLE">1000</Value>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cbc:PayableRoundingAmount</Term>
            <Name>Rounding amount</Name>
            <Description>The amount to be added to the invoice total to round the amount to be paid. </Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-114</Reference>

            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>
                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            <Value type="EXAMPLE">0</Value>
        </Element>
        
        <Element>
            <Term>cbc:PayableAmount</Term>
            <Name>Amount due for payment</Name>
            <Description>The outstanding amount that is requested to be paid. </Description>
            <DataType>Amount</DataType>
            <Reference type="BUSINESS_TERM">BT-115</Reference>
            <Reference type="RULE">BR-15</Reference>
            <Reference type="RULE">BR-CO-16-GST-SG</Reference>

            
            <Attribute>
                <Term>currencyID</Term>
                <Description>Mandatory attribute. Use BT-5</Description>
                <Reference type="RULE">PEPPOL-EN16931-CL007</Reference>
                <Reference type="RULE">PEPPOL-EN16931-R051</Reference>
                <Reference type="CODE_LIST">ISO4217</Reference>
                <Value type="EXAMPLE">SGD</Value>
            </Attribute>
            <Value type="EXAMPLE">3500</Value>
        </Element>
    </Element>